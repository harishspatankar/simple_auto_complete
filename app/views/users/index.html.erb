<h1>Users</h1>
<div>
  <table border="1" id="user-data-table">
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
    </tr>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
    </tr>
    <% end %>
  </table>
</div>
<br>
<br>
<div>
  <form methods='get' action='/search_suggestions'>
    <input type= "text" id="search" name= "search" placeholder="search user" autocomplete="off">
    <ul id="result"></ul>
  </form>
</div>
</html>
